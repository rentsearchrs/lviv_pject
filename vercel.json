{
    "version": 2,
    "builds": [
        {
            "src": "parser/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "parser/main.py",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
                "Access-Control-Allow-Headers": "Authorization, Content-Type"
            }
        }
    ],
    "trailingSlash": false
}
