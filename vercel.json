{
    "version": 2,
    "builds": [
        {
            "src": "/parser/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "parser/main.py"
        }
    ]
    "functions": {
        "api/*": {
            "runtime": "edge"
        }
    }
}
