{
    "version": 2,
    "builds": [
        {
            "src": "parser/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "parser/main.py",
            "headers": {
                "Access-Control-Allow-Origin": "https://app-lemon-beta-90.vercel.app",
                "Access-Control-Allow-Methods": "*",
                "Access-Control-Allow-Headers": "*"
            }
        }
    ]
}
