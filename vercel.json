{
    "version": 2,
    "builds": [
        {
            "src": "parser/main.py",
            "use": "@vercel/python",
            "config": {
                "startCommand": "uvicorn parser.main:app --host 0.0.0.0 --port 8000"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "parser.main:app",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
      